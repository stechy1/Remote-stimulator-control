<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EditableSeekbar">
        <!-- Text, který se zobrazí jako popisek seek baru -->
        <attr name="label" format="string" />
        <!-- Maximální hodnota seekbaru -->
        <attr name="max" format="integer" />
        <!-- Aktuální progres seekbaru -->
        <attr name="progress" format="integer" />
    </declare-styleable>

</resources>